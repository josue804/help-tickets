{{yield}}

<h1>Submit your ticket!</h1>
<form>
  <div class="form-group">
    <label for="name">Your Name</label>
    {{input id="name" value=name class="form-control"}}
  </div>
  <div class="form-group">
    <label for="question">Your Question</label>
    {{input id="question" value=question class="form-control"}}
  </div>
  <div class="form-group">
    <label for="subject">What About?</label>
    {{input id="subject" value=subject class="form-control"}}
  </div>
  {{input id="timeSubmitted" value="{{time-submitted}}" type="hidden"}}
  <button class="btn btn-primary btn-lg" type="submit" {{action "createTicket"}}>Submit</button>
</form>
